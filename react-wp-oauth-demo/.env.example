# WordPress OAuth2 Demo Configuration
VITE_WP_BASE_URL=https://your-wordpress-site.com
VITE_OAUTH_CLIENT_ID=your-client-id

# Client secret is optional when using PKCE (recommended for browser-based apps)
# Only use client_secret for server-side applications
VITE_OAUTH_CLIENT_SECRET=

VITE_OAUTH_REDIRECT_URI=http://localhost:5175/callback
VITE_DEBUG=true